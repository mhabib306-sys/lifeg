{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Homebase",
  "version": "4.13.3",
  "identifier": "com.homebase.desktop",
  "build": {
    "frontendDist": "https://mhabib306-sys.github.io/lifeg/",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev"
  },
  "app": {
    "windows": [
      {
        "title": "Homebase",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
